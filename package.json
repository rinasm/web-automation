{
  "name": "playwright-test-generator",
  "version": "1.0.0",
  "description": "Electron desktop app for generating Playwright test code visually",
  "main": "dist/electron/electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:react\" \"npm run dev:electron\"",
    "dev:react": "vite",
    "dev:electron": "tsc -p tsconfig.electron.json && electron .",
    "build": "npm run build:react && npm run build:electron",
    "build:react": "vite build",
    "build:electron": "tsc -p tsconfig.electron.json",
    "package": "electron-builder",
    "test": "playwright test",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "keywords": [
    "playwright",
    "test-automation",
    "electron",
    "code-generation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-electron": "^0.28.2"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@devicefarmer/adbkit": "^3.2.6",
    "@playwright/test": "^1.40.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/ws": "^8.18.1",
    "active-win": "^9.0.0",
    "appium": "^2.19.0",
    "appium-xcuitest-driver": "^7.35.1",
    "bonjour-service": "^1.3.0",
    "chrome-remote-interface": "^0.33.2",
    "get-windows": "^9.2.3",
    "groq-sdk": "^0.34.0",
    "lucide-react": "^0.294.0",
    "node-fetch": "^2.7.0",
    "nutjs": "^0.0.1",
    "openai": "^6.8.1",
    "playwright": "^1.40.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^16.1.0",
    "robotjs": "^0.6.0",
    "uiohook-napi": "^1.5.4",
    "webdriverio": "^9.20.0",
    "whisper-webgpu": "^0.10.0",
    "ws": "^8.18.3",
    "xml2js": "^0.6.2",
    "zustand": "^4.4.7"
  },
  "build": {
    "appId": "com.playwright.testgenerator",
    "productName": "Playwright Test Generator",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
